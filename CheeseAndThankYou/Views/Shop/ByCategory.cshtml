@model IEnumerable<Product>
@{
    ViewData["Title"] = "Shop for our " + @ViewData["Category"] + " Cheeses";
}
<h1>ALL of these are really good...</h1>

<ul>
    @*
    <li>@ViewData["Category"] Cheese 1</li>
    <li>@ViewData["Category"] Cheese 2</li>
    <li>@ViewData["Category"] Cheese 3</li>
    *@
    @foreach (var product in Model)
    {
        <li>@product.Name</li>
    }

</ul>
